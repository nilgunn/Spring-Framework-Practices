<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.2.xsd">


	<bean id="beancalisan" class="paket.Calisan" autowire="byType">
		<property name="adi" value="Ramin"></property>
		<property name="soyadi" value="Orucov"></property>
	</bean>
	<bean id="beanadres" class="paket.Adres">
		<property name="tamadres" value="İstanbul/Beşiktaş"></property>
	</bean>
</beans>
